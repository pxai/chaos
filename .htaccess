RewriteEngine on
RewriteBase /chaos/

RewriteRule ^([a-z\_]+)$ index.php?p=$1 [L]
RewriteCond $1 !^(index\.php|js|css|images|robots\.txt|favicon\.ico) 
RewriteRule ^(.*)$ index.php?p=$1 [L] 

